	<element name="Tool Expertise (Bagpipes)" type="Class Feature" source="Player’s Handbook" id="ID_FLP_LRRP_EXPERTISE_TOOL_BAGPIPES">
		<supports>Expertise, Tool Expertise</supports>
		<requirements>ID_PROFICIENCY_TOOL_PROFICIENCY_BAGPIPES</requirements>
		<description>
			<p>Your proficiency bonus is doubled for any Bagpipes check you make.</p>
			<div class="reference">
				<div element="ID_WOTC_SRD_INSTRUMENT_BAGPIPES" />
			</div>
		</description>
		<sheet>
        	<description>Your proficiency bonus is doubled for any Bagpipes check you make.</description>
		</sheet>
		<rules>
			<stat name="bagpipes:proficiency" value="proficiency" bonus="double" />
		</rules>
	</element>

